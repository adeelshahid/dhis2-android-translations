msgctxt "app_name"
msgid "DHIS2 Event Capture"
msgstr "DHIS2 Event Capture"

#. Navigation drawer strings
msgctxt "drawer_item_events"
msgid "Events"
msgstr "Eventos"

msgctxt "filter_title"
msgid "Filter"
msgstr "Filtro"

msgctxt "filter_sections"
msgid "Filter sections"
msgstr "Secções de filtro"

msgctxt "option_confirm"
msgid "Ok"
msgstr "Ok"

#. Logout warning dialog:
msgctxt "warning_logout_body"
msgid ""
"You will be logged out and any files that have been saved on this device "
"for offline viewing will be deleted."
msgstr ""
"Será desconectado e quaisquer files que tenham sido gravados neste "
"dispositivo para a visualização offline serão apagados"

msgctxt "warning_logout_header"
msgid "Are you sure?"
msgstr "Tem a certeza?"

msgctxt "warning_logout_confirm"
msgid "Log out"
msgstr "Terminar a sessão"

msgctxt "warning_logout_dismiss"
msgid "close"
msgstr "fechar"

#. Error/warning messages
msgctxt "title_error"
msgid "Error"
msgstr "Erro"

msgctxt "title_error_unexpected"
msgid "Unexpected error"
msgstr "Erro inesperado"

msgctxt "error_not_found"
msgid "Server not found."
msgstr "O servidor não foi encontrado."

msgctxt "error_unauthorized"
msgid "Wrong username or password."
msgstr "Utilizador ou senha inválidos."

#. Delete Event confirmation dialog
msgctxt "delete"
msgid "Delete"
msgstr "Apagar"

msgctxt "delete_report_entity_dialog_title"
msgid "Delete this event?"
msgstr "Apagar este evento?"

msgctxt "delete_report_entity_dialog_message"
msgid ""
"The event will be lost forever. You will have to create a new event and "
"re-enter any information."
msgstr ""
"O evento será perdido para sempre. Terá de criar um novo evento e "
"reintroduzir mais informação."

msgctxt "report_entity_deletion_error"
msgid "Unable to delete Event"
msgstr "Não é possível apagar evento"

#. Data entry screen prompts
msgctxt "undo"
msgid "Undo"
msgstr "Desfazer"

msgctxt "complete"
msgid "Complete"
msgstr "Terminado"

msgctxt "incomplete"
msgid "Incomplete"
msgstr "Incompleto"

#. Syncing
msgctxt "sync_complete_notification_title"
msgid "Sync complete"
msgstr "Sincronização completa"

msgctxt "sync_complete_notification_content"
msgid "Your DHIS2 data has been synchronized"
msgstr "Os seus dados do DHIS2 foram sincronizados"

msgctxt "sync_failed_notification_title"
msgid "Sync failed"
msgstr "A Sincronização falhou"

msgctxt "sync_failed_notification_content"
msgid "Your DHIS2 data has not been synchronized"
msgstr "Os seus dados do DHIS2 não foram sincronizados"

msgctxt "sync_in_progress_notification_title"
msgid "Syncing"
msgstr "A sincronizar..."

msgctxt "sync_in_progress_notification_content"
msgid "Your DHIS2 data is synchronizing"
msgstr "Os seus dados DHIS2 estão sendo sincronizados"

